<% content_for :title do %>NYC Dashboard<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="clock" data-view="Clock" style="background-color:#A60272;"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="2">
      <div data-id="calendar_events_nyc" data-pre="0" data-view="Calendar" style="background-color:#01484A;"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="twitter_pictures" data-view="TwitterPictures" style="background-color:#C09900;"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="pivot_faces_nyc" data-view="PivotFaces" style="background-color:#A64723;"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="forecast-nyc" data-view="Forecast" style="background-color:#009198;"></div>
    </li>
  </ul>
</div>

